page{
  background: #F1F1F1;
}
.experience{
  padding: 32rpx 40rpx;
  background: #fff;
}
.experience .bgbar{
  width: 100%;
  height: 8rpx;
  border-radius: 4rpx;
  background: #E5E5E5;
  margin-top: 50rpx;
  position: relative;
}
.experience .bgbar .upbar{
  width: 80%;
  height: 8rpx;
  border-radius: 4rpx;
  background: #98A4B2;
}
.experience .bgbar image{
  width: 52rpx;
  height: 52rpx;
  position: absolute;
  top: -20rpx;
  left: 80%;
}
.experience .upgrade {
  font-size: 24rpx;
  color: #6B6B6B;
  text-align: center;
  margin-top: 42rpx;
}
.privilege{
  background: #fff;
  padding: 32rpx 24rpx;
  margin-top: 12rpx;
}
.privilege .title{
  font-size: 28rpx;
}
.privilege .coupon{
  margin-top: 32rpx;
}
.privilege .coupon .icon_word{
  float: left;
}
.privilege .coupon image{
  width: 48rpx;
  height: 48rpx;
  float: left;
}
.privilege .coupon .word{
  font-size: 28rpx;
  float: left;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 24rpx;
}
.btn{
  font-size: 24rpx;
  float: right;
  padding: 2rpx 16rpx;
  border-radius: 8rpx;
  background: #FF5000;
  color: #fff;
  margin-top: 4rpx;
}
.upprivilege{
  background: #fff;
  padding: 32rpx 24rpx;
  margin-top: 12rpx;
}
.upprivilege .title{
  font-size: 28rpx;
}
.upprivilege .upprivilegelist{
  padding: 32rpx 0;
  border-bottom: 2rpx solid #E5E5E5;
}
.upprivilege .upprivilegelist image{
  width: 48rpx;
  height: 48rpx;
  float: left;
}
.upprivilege .upprivilegelist .word{
  font-size: 28rpx;
  float: left;
  height: 48rpx;
  line-height: 48rpx;
  margin-left: 24rpx;
}